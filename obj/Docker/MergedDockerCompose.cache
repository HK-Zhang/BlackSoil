services:
  blacksoil.aspnet:
    build:
      context: F:\GitRepos\BlackSoil
      dockerfile: BlackSoil.AspNet/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: blacksoilaspnet
    ports:
    - target: 80
  blacksoil.blazor.server:
    build:
      context: F:\GitRepos\BlackSoil\BlackSoil.Blazor.Server
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: blacksoilblazorserver
    ports:
    - target: 80
version: '3.4'